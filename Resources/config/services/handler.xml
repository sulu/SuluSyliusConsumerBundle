<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Sulu\Bundle\SyliusConsumerBundle\Model\Product\Handler\SynchronizeProductHandler">
            <argument type="service" id="Sulu\Bundle\SyliusConsumerBundle\Model\Product\ProductRepositoryInterface"/>
            <argument type="service" id="Sulu\Bundle\SyliusConsumerBundle\Model\Product\ProductVariantRepositoryInterface"/>

            <tag name="messenger.message_handler"/>
        </service>

        <service id="Sulu\Bundle\SyliusConsumerBundle\Model\Product\Handler\RemoveProductHandler">
            <argument type="service" id="Sulu\Bundle\SyliusConsumerBundle\Model\Product\ProductRepositoryInterface"/>

            <tag name="messenger.message_handler"/>
        </service>
    </services>
</container>
